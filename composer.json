{
  "name": "glebsky/simple-queue",
  "description": "Simple queues for your php application",
  "version": "1.0.0",
  "keywords": [
    "php",
    "queue",
    "simple-queue",
    "database queue"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Glebsky\\SimpleQueue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Glebsky\\SimpleQueueTest\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Hlib",
      "email": "lotamountofsoul@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "scripts": {
    "test": "php vendor/bin/phpunit --colors=always",
    "test-coverage": "php vendor/bin/phpunit --coverage-text --coverage-html ./docs/tmp/"
  }
}
